{
  "name": "setbased/php-stratum-backend",
  "description": "PhpStratum Backend: The glue between PhpStratum and backends for RDBMS",
  "keywords": [
    "PhpStratum"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "symfony/console": "^6.2.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC3",
    "phpstan/phpstan": "^1.9.14"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\Backend\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
