{
  "name": "setbased/php-stratum-backend",
  "description": "PhpStratum Backend: The glue between PhpStratum and backends for RDBMS",
  "keywords": [
    "PhpStratum"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "symfony/console": "^5.0"
  },
  "require-dev": {
    "phing/phing": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\Backend\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\Backend\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
